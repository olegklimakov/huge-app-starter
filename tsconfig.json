{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "todo": [
        "dist/todo/todo",
        "dist/todo"
      ],
      "@libs/todo": [
        "dist/libs/todo/libs-todo",
        "dist/libs/todo",
        "dist/libs/todo/libs-todo",
        "dist/libs/todo"
      ],
      "@libs/my-library": [
        "dist/libs/my-library/libs-my-library",
        "dist/libs/my-library"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}